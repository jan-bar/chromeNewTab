{
  "name": "NewTab",
  "description": "Janbar Start Page",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "janbar.html"
  },
  "icons": {
    "128": "img/p128.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval' https://www.baidu.com; object-src 'self' ;",
  "manifest_version": 2
}